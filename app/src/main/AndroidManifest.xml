<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gonews">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoNews">
        <activity
            android:name=".Activity_Health"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity_International"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity_Technology"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity_Science"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity_Entertainment"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity_Sports"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>